{
    "name": "planit",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "eslint": "eslint",
        "prettier": "prettier --write '**/*.{ts,js,html}'",
        "format": "eslint --fix && prettier --write '**/*.{ts,js,html}'",
        "format:check": "eslint --max-warnings=0 && prettier --check '**/*.{ts,js,html}'",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "unitttest": "vitest run --passWithNoTests",
        "integtest": "playwright test",
        "test": "pnpm run unitttest && pnpm run integtest",
        "vibe": "vibe-kanban"
    },
    "devDependencies": {
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^10.0.1",
        "@eslint/markdown": "^7.5.1",
        "@html-eslint/eslint-plugin": "^0.54.2",
        "@html-eslint/parser": "^0.54.0",
        "@playwright/test": "^1.58.2",
        "@storybook/addon-a11y": "^10.2.8",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/addon-interactions": "^8.6.14",
        "@storybook/addon-links": "^10.2.8",
        "@storybook/html": "^10.2.8",
        "@storybook/html-vite": "^10.2.8",
        "@tailwindcss/node": "^4.1.18",
        "@tailwindcss/vite": "^4.1.18",
        "@types/node": "^25.2.2",
        "@vite-pwa/assets-generator": "^1.0.2",
        "eslint": "^9.39.2",
        "eslint-plugin-better-tailwindcss": "^4.1.1",
        "globals": "^17.3.0",
        "prettier": "3.8.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "storybook": "^10.2.8",
        "tailwindcss": "^4.1.18",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.55.0",
        "vibe-kanban": "^0.1.8",
        "vite": "^7.1.9",
        "vite-plugin-pwa": "^1.1.0",
        "vitest": "^3.2.4",
        "workbox-window": "^7.4.0"
    },
    "resolutions": {
        "sharp": "^0.33.5",
        "sharp-ico": "^0.1.5"
    }
}
